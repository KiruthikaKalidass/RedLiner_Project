
<div class="calcite">

    <link rel="stylesheet" href="https://js.arcgis.com/3.27/esri/themes/calcite/dijit/calcite.css">
    <link rel="stylesheet" href="https://js.arcgis.com/3.27/esri/themes/calcite/esri/esri.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">


    <div class="header" style="width:100%;background-color:burlywood;padding-top:10px;padding-bottom:10px;text-align:center;color:firebrick;">
        <img src="data:image/png;base64," style="float:left" />
        <canvas id="myCanvas" width="200" height="70" style="color:firebrick;"></canvas>
        <script>
            var c = document.getElementById("myCanvas");
            var ctx = c.getContext("2d");

            ctx.font = "30px Verdana";


            var gradient = ctx.createLinearGradient(0, 0, c.width, 0);
            gradient.addColorStop("0", "magenta");
            gradient.addColorStop("0.5", "blue");
            gradient.addColorStop("1.0", "red");


            ctx.strokeStyle = gradient;
            ctx.strokeText("REDLINER", 10, 50);
        </script>
    </div>
    <div class="main">
        <div class="content" style="width:82%;float: left;border:solid;height:543px;border-width:2px;">
            <div data-dojo-type="dijit/layout/BorderContainer" data-dojo-props="design:'headline', gutters:false" style="width: 100%; height: 100%; margin:0;">
                <div id="map" data-dojo-type="dijit/layout/ContentPane" data-dojo-props="region:'center'" style="padding:0">
                </div>
                <div id="search"></div>
                <div id="LocateButton"></div>


            </div>
        </div>

        <div class="right" style="width:18%; border:solid;border-color:white; float: right; ">
            <div class="tab">
                <button class="tablinks" onclick="show(event, 'Activity')" style="width:125px;">Activity</button>

                <button class="tablinks" onclick="show(event, 'RedLiner')" style="width:112px;">RedLiner</button>

            </div>
            <div id="Activity" class="tabcontent" style="color:gray;">

                <div data-dojo-type="dijit/TitlePane" data-dojo-props="title:'Shapes',open:false " style="color:black;background-color:gray;">
                    <div data-dojo-props="dijit/layout/ContentPane" style="width:250px; height:129px;overflow:auto;">
                        <div>
                            &nbsp;&nbsp; <button data-dojo-type="dijit/form/Button">Point</button>
                            &nbsp;&nbsp; <button data-dojo-type="dijit/form/Button">Multi Point</button>
                            &nbsp;&nbsp; <button data-dojo-type="dijit/form/Button">Line</button>
                        </div>
                        <br />
                        <div>
                            &nbsp;&nbsp;<button data-dojo-type="dijit/form/Button">Polyline</button>
                            &nbsp;&nbsp; <button data-dojo-type="dijit/form/Button">Polygon</button>

                        </div>
                        <br />
                        <div>
                            &nbsp;&nbsp; <button data-dojo-type="dijit/form/Button">Freehand Polyline</button>
                            &nbsp;&nbsp; <button data-dojo-type="dijit/form/Button">Triangle</button>
                        </div>
                        <br />
                        <div>
                            &nbsp;&nbsp; <button data-dojo-type="dijit/form/Button">Freehand Polygon</button>
                            &nbsp;&nbsp; <button data-dojo-type="dijit/form/Button">Circle</button>
                        </div>
                        <br />
                        &nbsp;&nbsp; <button data-dojo-type="dijit/form/Button">Ellipse</button>
                        &nbsp;&nbsp; <button data-dojo-type="dijit/form/Button">Arrow</button>

                    </div>
                </div>
                <div id="titlePane" data-dojo-type="dijit/TitlePane" data-dojo-props="title:'Measurement', open:false" style="color:black;background-color:gray;">
                    <div data-dojo-props="dijit/layout/ContentPane" style="width:231px; height:213px;overflow:auto;">
                        <div id="measurement"></div>
                    </div>

                </div>
                <div data-dojo-type="dijit/TitlePane"
                     data-dojo-props="title:'BaseMapGallery', open:false" style="color:black;background-color:white;">
                    <div data-dojo-type="dijit/layout/ContentPane" style="width:250px; height:103px; overflow:auto;">
                        <div id="basemapGallery"></div>
                    </div>
                </div>
                <div data-dojo-type="dijit/TitlePane"
                     data-dojo-props="title:'PrintMap', open:false" style="color:black;background-color:gray;">
                    <div data-dojo-type="dijit/layout/ContentPane" style="width:250px; height:30px; overflow:auto;">
                        <div id="printer"></div>
                    </div>
                </div>

            </div>
            <div id="RedLiner" class="tabcontent" style=" height:487px;">


                <div class="container">
                    <button type="button"  style="padding-left:67px;padding-right:69px;background-color:#009688;border-color:black;" class="btn btn-primary" data-toggle="modal" data-target="#myModal" onclick="clearTextBox();">Add Project</button><br /><br />

                    <table class="table table-bordered table-hover">
                        <thead style="color:blue">
                            <tr>

                                <th>
                                    <b>ID</b>
                                </th>
                                <th>
                                    <b>Name</b>
                                </th>
                                <th>
                                    <b>Actions</b>
                                </th>
                            </tr>
                        </thead>
                        <tbody class="tbody"></tbody>
                    </table>
                    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    @*<button type="button" class="close" data-dissmiss="modal"><span aria-hidden="true">×</span></button>*@
                                    <button type="button" class="close" data-dismiss="modal">×</button>
                                    <h4 class="modal-title" id="myModalLabel">Add Project</h4>
                                </div>
                                <div class="modal-body">
                                    <form>

                                        <div class="form-group">
                                            <label for="ProjectId">ID</label>
                                            <input type="text" class="form-control" id="ProjectID" placeholder="Id" />
                                        </div>
                                        <div class="form-group">
                                            <label for="Name">Name</label>
                                            <input type="text" class="form-control" id="Name" placeholder="Name" />
                                        </div>
                                    </form>
                                    <div class="modal-footer">

                                        <button type="button" class="btn btn-primary" id="btnAdd" onclick="return Add();">Add</button>
                                        <button type="button" class="btn btn-primary" id="btnUpdate" style="display:none;" onclick="Update();">Update</button>
                                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                    </div>
                                </div>
                            </div>

                        </div>

                        <script>
                            function show(evt, Activity) {
                                var i, tabcontent, tablinks;
                                tabcontent = document.getElementsByClassName("tabcontent");
                                for (i = 0; i < tabcontent.length; i++) {
                                    tabcontent[i].style.display = "none";
                                }
                                tablinks = document.getElementsByClassName("tablinks");
                                for (i = 0; i < tablinks.length; i++) {
                                    tablinks[i].className = tablinks[i].className.replace("active", "");
                                }
                                document.getElementById(Activity).style.display = "block";
                                evt.currentTarget.className += "active";
                            }
                        </script>

                    </div>

                </div>

            </div>
        </div>
    </div>
    <div class="footer" style=" position: absolute; right: 0;bottom:-12px;left: 0;background-color:burlywood;color:firebrick;">
        <p style="float:left;">Email-Id:Kiruthikageogri@gmail.com</p><p style="float:right;">GISbiz technologies pvt ltd</p>
    </div>
</div>        
           